<div class="user_module mt-3">
    <img class="twitter_img" src="<%= user.image_url_https %>">
    <span>
        <div class="utwitter_name">
            <%= user.twitter_name %>
            <span class="twitter_id"><a href="https://twitter.com/@<%= user.twitter_id %>">@<%= user.twitter_id %></a></span>
        </div>
        <div class="skype">Skype: <a class="copylink" href="<%= user.skype_id %>"><%= user.skype_id %></a></div>
    </span>
</div>
